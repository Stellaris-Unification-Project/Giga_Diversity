entity = {
	name = "vhab_planet_01_entity"
	pdxmesh = "planet_no_pole_no_clouds_mesh"
	
	 
	meshsettings = {
	name = "planet_geosphereShape"
	shader = "PdxMeshPlanetEmissive"
	texture_diffuse = "mhab_01_diffuse.dds"
	texture_normal = "mhab_01_normals.dds"
	texture_specular = "mhab_01_specular.dds"
	}
	
	default_state = "idle"
